[configure]
name                    = "10.1"
logging_level           = "info"

[configure.audio]
audio                   = "jack"
midi                    = "jack"
sampling_rate           = 44100
buffer_size             = 512
channel_count           = 2

{% set fade_in          = 4 %}
{% set fade_out         = 4 %}

[configure.module.string.0]
input_channel_index     = 0
com_port                = "/dev/ttyACM0"
arduino_instance_id     = 1
pin                     = 3
frequency               = "parameter.string_0_frequency"
fade_in_duration        = {{ fade_in }}
fade_out_duration       = {{ fade_out }}

[configure.module.value.string_0_child_decibel]
# We have a very quiet signal
value                   = 40

[configure.module.parameter.string_0_frequency]
fade_in_duration        = {{ fade_in }}
fade_out_duration       = {{ fade_out }}

[configure.module.mixer.strings]
audio_input_0           = "string.0"

[configure.module.butterworth_highpass_filter.strings]
audio_input             = "mixer.strings"

[configure.module.value.butterworth_highpass_filter_strings_child_frequency]
value                   = 90

[configure.module.butterworth_lowpass_filter.strings]
audio_input             = "butterworth_highpass_filter.strings"

[configure.module.value.butterworth_lowpass_filter_strings_child_frequency]
value                   = 15000

[configure.module.mixer.master]
send_to_physical_output = true
audio_input_0           = "butterworth_lowpass_filter.strings"



[cue.240.string.0]

[cue.240.parameter.string_0_frequency]
value = 240



[cue.120.string.0]

[cue.120.parameter.string_0_frequency]
value = 120



[cue.60.string.0]

[cue.60.parameter.string_0_frequency]
value = 60



[cue.30.string.0]

[cue.30.parameter.string_0_frequency]
value = 30
