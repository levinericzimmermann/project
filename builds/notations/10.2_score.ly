#(set-default-paper-size "b4" 'landscape)
\header
{
    tagline = ""
}
\paper
{
    	oddHeaderMarkup = \markup \fill-line { " " }
	    evenHeaderMarkup = \markup \fill-line { " " }
	    oddFooterMarkup = \markup {
	      \fill-line {
	        \typewriter \fontsize #-1
	        \fromproperty #'page:page-number-string
	      }
	    }
	    evenFooterMarkup = \markup {
	      \fill-line {
	        \typewriter \fontsize #-1
	        \fromproperty #'page:page-number-string
	      }
	    }

	    print-first-page-number = ##t





	top-margin = 1.5\cm



	bottom-margin = 1.5\cm



	left-margin = 2\cm




	foot-separation = 0\cm



	head-separation = 0\cm




	line-width = 30.5\cm




	before-title-space = 0\cm



	after-title-space = 0\cm



	between-title-space = 0\cm




	page-top-space = 0\cm






	% Deactivated
	% system-separator-markup = \markup \fill-line { \override #'(span-factor . 1/16) \draw-hline }

	system-system-spacing = #'(
	    (basic-distance . 6)
	    (padding . 1)
	)
	score-system-spacing = #'(
	    (basic-distance . 0)
	    (padding . 0)
	)
	markup-system-spacing = #'(
	    (basic-distance . 0)
	    (padding . 0)
	)

	#(define fonts
	    (make-pango-font-tree "Liberation Mono" "Liberation Mono" "Liberation Mono"
	    (/ staff-height pt 20))
	)

	#(define fonts
	  (set-global-fonts
