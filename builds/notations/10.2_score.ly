    	  \override StaffGrouper.staffgroup-staff-spacing.minimum-distance = #7
    	  \override StaffGrouper.staffgroup-staff-spacing.padding = #1
    	  \override StaffGrouper.staffgroup-staff-spacing.stretchability = #1
    	}

    	\context {
    	  \Staff
    	  \omit BarLine
    	  \omit TimeSignature

